.container {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.header {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: 0 20rpx;
    height: 100rpx;
    box-sizing: border-box;
}

.movie,
.essay,
.music {
    width: 100%;
}